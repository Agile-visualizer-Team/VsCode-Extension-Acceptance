{
    "name": "@agilevisualizerteam/visualizer-asp",
    "displayName": "visualizer-asp",
    "description": "",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:hello-extension.helloWorld",
        "onCommand:hello-extension.time.current",
        "onCommand:hello-extension.cmd"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "hello-extension.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "hello-extension.time.current",
                "title": "Time"
            },
            {
                "command": "hello-extension.cmd",
                "title": "Render Matrix"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "webpack": "^5.74.0",
        "typescript": "^4.8.4",
        "ts-loader": "^9.4.1",
        "table": "^6.8.1",
        "node-html-to-image": "^3.2.4",
        "mocha": "^10.1.0",
        "glob": "^8.0.3",
        "eslint": "^8.26.0",
        "@vscode/test-electron": "^2.2.0",
        "@typescript-eslint/parser": "^5.42.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@types/vscode": "^1.73.0",
        "@types/node": "16.x",
        "@types/mocha": "^10.0.0",
        "@types/glob": "^8.0.0"
    }
}
